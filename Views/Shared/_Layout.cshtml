@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Prueba de Bootstrap - Table</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/lib/bootstrap-table/dist/bootstrap-table.min.css">
    <link rel="stylesheet" href="~/lib/fontawesome/css/all.min.css" />
    <link rel="stylesheet" href="~/css/site.css">
    <script src="~/lib/jquery/dist/jquery-3.5.1.min.js"></script>
    <style>
        .select,
        #locale {
            width: 100%;
        }

        .like {
            margin-right: 10px;
        }
    </style>
</head>
<body>

    <div class="tab-content">Hoy es @DateTime.Now</div>
    @RenderBody()

    <script src="~/lib/jquery/dist/popper.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>

    <script src="~/lib/bootstrap-table/dist/bootstrap-table.min.js"></script>
    <script src="~/lib/bootstrap-table/dist/bootstrap-table-locale-all.min.js"></script>

    <script src="~/lib/bootstrap-table-export/dist/FileSaver/FileSaver.min.js"></script>
    <script src="~/lib/bootstrap-table-export/dist/js-xlsx/xlsx.core.min.js"></script>
    <script src="~/lib/bootstrap-table-export/dist/es6-promise/es6-promise.auto.min.js"></script>
    <script src="~/lib/bootstrap-table-export/dist/html2canvas/html2canvas.min.js"></script>

    <script src="~/lib/bootstrap-table-export/dist/jsPDF/jspdf.min.js"></script>
    <script src="~/lib/bootstrap-table-export/dist/jsPDF-AutoTable/jspdf.plugin.autotable.js"></script>

    <script src="~/lib/bootstrap-table-export/dist/tableExport.min.js"></script>
    <script src="~/lib/bootstrap-table/dist/extensions/export/bootstrap-table-export.min.js"></script>

    @RenderSection("Scripts", required: false)

</body>
</html>